{
  "name": "giftpool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "dev:coverage": "COVERAGE=1 nyc next dev -p 8081",
    "migrate": "bun prisma migrate dev",
    "seed": "bun run src/db/seed.ts",
    "test": "bun run migrate && bun run seed && bun test",
    "lint": "bun biome check --apply .",
    "type": "bun tsc",
    "e2e": "bun run migrate && bun run seed && playwright test",
    "e2e:ci": "bun run migrate && bun run seed && CI=true playwright test",
    "e2e:cov": "bun run migrate && bun run seed && playwright test; nyc report",
    "e2e:cov:ci": "bun run migrate && bun run seed && CI=true playwright test; nyc report",
    "build": "NODE_ENV=production next build",
    "vercel-build": "bun run migrate && bun run seed && bun run build",
    "start": "next start -p 8080",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "5.15.6",
    "@mui/material": "5.15.6",
    "@prisma/client": "5.8.1",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.17",
    "next": "14.1",
    "next-pwa": "^5.6.0",
    "pino": "^8.17.2",
    "postcss": "8.4.33",
    "postgres": "^3.4.3",
    "prisma": "5.8.1",
    "react": "canary",
    "react-dom": "canary",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@happy-dom/global-registrator": "^13.3.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "1.41.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.0.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "dotenv-cli": "^7.3.0",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "nyc": "^15.1.0",
    "playwright-test-coverage": "^1.2.12",
    "webpack": "^5.90.0"
  }
}
